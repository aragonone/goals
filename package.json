{
  "name": "aragon-goals",
  "version": "1.0.0",
  "bin": "./bin/aragon-goals",
  "scripts": {
    "build": "fpack build --development --preprocess='^src/.+\\.js$:babel-loader?filename=.babelrc' ./src/index.js",
    "watch": "fpack watch --development --preprocess='^src/.+\\.js$:babel-loader?filename=.babelrc' ./src/index.js",
    "serve": "fpack serve --development --port 4000 --preprocess '^src/.+\\.js$:babel-loader?filename=.babelrc' ./src/index.js"
  },
  "dependencies": {
    "@babel/polyfill": "^7.2.5",
    "react": "^16.8.0",
    "react-dom": "^16.8.0",
    "react-dropzone": "^8.0.4",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "@babel/core": "^7.2.2",
    "@babel/plugin-proposal-class-properties": "^7.3.0",
    "@babel/preset-env": "^7.3.1",
    "@babel/preset-react": "^7.0.0",
    "babel-loader": "^8.0.5",
    "babel-plugin-styled-components": "^1.10.0",
    "fpack": "^0.6.8",
    "loader-runner": "^3.0.0",
    "uglify-js": "^3.4.9"
  }
}
